{% extends 'base.html.twig' %}


{% block title %}Page non trouvé{% endblock %}

{% block body %}
<section class="erreur">
    <div class="img_erreur">
        <img src="{{ asset('image/cupcake_404.jpg') }}" alt="">
    </div>
    <div class="text_erreur">
        <h1>Page non trouvée</h1>
        <p>La page que vous cherchez n'a pas été trouvée</p>
        <p>Code de l'erreur : {{ status_code }}</p>
    </div>
</section>

{% endblock %}